{
  "name": "dentistry-exchange-monorepo",
  "version": "1.14.0",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "clean": "turbo run clean && rm -rf node_modules",
    "db:push": "turbo run db:push --filter=@dentex/database",
    "seed": "npx tsx scripts/seed-data.ts",
    "test:verify": "echo 'See scripts/verify-flow.md for manual testing steps.'",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "turbo": "^1.10.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "npm@9.6.0"
}